{
  "name": "demborg.github.io",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://demborg.github.io",
  "dependencies": {
    "@types/jest": "24.0.18",
    "@types/node": "12.7.11",
    "@types/react": "^16.9.5",
    "@types/react-dom": "16.9.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-scripts": "3.2.0",
    "typescript": "3.6.3"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -b master -d build",
